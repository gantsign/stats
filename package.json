    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@types/react-measure": "^2.0.8",
    "@types/react-plotly.js": "^2.5.2",
    "@types/semver": "^7.3.13",
    "axios": "^1.2.1",
