    "gts": "^3.1.0",
    "json": "^11.0.0",
    "prettier-package-json": "^2.6.3",
    "react-scripts": "5.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14 <15"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
