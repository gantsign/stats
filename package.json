    "gts": "^3.1.1",
    "json": "^11.0.0",
    "prettier-package-json": "^2.7.0",
    "react-scripts": "5.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14 <15"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
